        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/spar/bido/open-peer-review -->

    <owl:NamedIndividual rdf:about="http://purl.org/spar/bido/open-peer-review">
