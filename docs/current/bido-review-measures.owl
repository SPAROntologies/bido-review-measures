    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/NET/rdflicense/cc-by4.0 -->

    <owl:NamedIndividual rdf:about="http://purl.org/NET/rdflicense/cc-by4.0"/>
    


    <!-- http://purl.org/spar/bido/a-na-wr -->

    <owl:NamedIndividual rdf:about="http://purl.org/spar/bido/a-na-wr">
        <rdf:type rdf:resource="http://purl.org/spar/bido/DiscreteReviewRatingScale"/>
        <rdfs:comment xml:lang="en">Discrete scale from Approved to Not Approved, including Aprproved With Reservations.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label xml:lang="en">Approved to Not Approved review rating scale</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/spar/bido/a-na-wr-app -->

    <owl:NamedIndividual rdf:about="http://purl.org/spar/bido/a-na-wr-app">
        <rdf:type rdf:resource="http://purl.org/spar/bido/ReviewRatingValue"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/a-r-ts-acc"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/dis-0-10-10"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/dis-0-5-4"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/dis-0-5-5"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/dis-n2-p2-pos1"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/dis-n2-p2-pos2"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/sa-sr-bl-acc"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/sa-sr-bl-sacc"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/sa-sr-nb-acc"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/sa-sr-nb-sacc"/>
        <skos:inScheme rdf:resource="http://purl.org/spar/bido/a-na-wr"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label xml:lang="en">Approved (Approved to Not Approved review rating scale)</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/spar/bido/a-na-wr-appwr -->

    <owl:NamedIndividual rdf:about="http://purl.org/spar/bido/a-na-wr-appwr">
        <rdf:type rdf:resource="http://purl.org/spar/bido/ReviewRatingValue"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/a-r-ts-minrev"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/dis-0-5-3"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/sa-sr-bl-wacc"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/sa-sr-nb-wacc"/>
        <skos:inScheme rdf:resource="http://purl.org/spar/bido/a-na-wr"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label xml:lang="en">Approved with reservations (Approved to Not Approved review rating scale)</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/spar/bido/a-na-wr-napp -->

    <owl:NamedIndividual rdf:about="http://purl.org/spar/bido/a-na-wr-napp">
        <rdf:type rdf:resource="http://purl.org/spar/bido/ReviewRatingValue"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/a-r-ts-rej"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/dis-0-10-0"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/dis-0-5-0"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/dis-0-5-1"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/dis-n2-p2-neg1"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/dis-n2-p2-neg2"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/sa-sr-bl-rej"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/sa-sr-bl-srej"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/sa-sr-nb-rej"/>
        <skos:closeMatch rdf:resource="http://purl.org/spar/bido/sa-sr-nb-srej"/>
        <skos:inScheme rdf:resource="http://purl.org/spar/bido/a-na-wr"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label xml:lang="en">Not approved (Approved to Not Approved review rating scale)</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/spar/bido/a-r-ts -->

    <owl:NamedIndividual rdf:about="http://purl.org/spar/bido/a-r-ts">
        <rdf:type rdf:resource="http://purl.org/spar/bido/DiscreteReviewRatingScale"/>
        <rdfs:comment xml:lang="en">Discrete scale from Accept to Reject, including pre-screening and two strikes rule</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label xml:lang="en">Accept to Reject, Two Strikes review rating scale</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/spar/bido/a-r-ts-acc -->

    <owl:NamedIndividual rdf:about="http://purl.org/spar/bido/a-r-ts-acc">
        <rdf:type rdf:resource="http://purl.org/spar/bido/ReviewRatingValue"/>
