    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2004/02/skos/core#prefLabel -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#prefLabel"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/spar/bido/hasMeasure -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/bido/hasMeasure"/>
    


    <!-- http://purl.org/spar/bido/holdsBibliometricDataInTime -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/bido/holdsBibliometricDataInTime"/>
    


    <!-- http://purl.org/spar/bido/isDefinedIn -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/bido/isDefinedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
        <rdfs:domain rdf:resource="http://purl.org/spar/bido/CategorialBibliometricData"/>
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
        <rdfs:comment>It links a ranking category with the ranking that defines it.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label>is defined in ranking</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/bido/isHigherThan -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/bido/isHigherThan">
        <owl:inverseOf rdf:resource="http://purl.org/spar/bido/isLowerThan"/>
        <rdfs:domain rdf:resource="http://purl.org/spar/bido/CategorialBibliometricData"/>
        <rdfs:range rdf:resource="http://purl.org/spar/bido/CategorialBibliometricData"/>
        <rdfs:comment>It links a ranking category with one that is less important.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label>is higher than</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/bido/isLowerThan -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/bido/isLowerThan">
        <rdfs:comment>It links a ranking category with one that is more important.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label>is lower than</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/bido/withBibliometricData -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/bido/withBibliometricData"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#closeMatch -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#closeMatch">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#inScheme -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#inScheme">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/spar/bido/hasConfidenceValue -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/bido/hasConfidenceValue">
        <rdfs:domain rdf:resource="http://purl.org/spar/bido/ReviewerConfidenceValue"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">The confidence value of expertise in the review topic given by the reviwer.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label xml:lang="en">has confidence value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/spar/bido/hasRatingValue -->

    <owl:DatatypeProperty rdf:about="http://purl.org/spar/bido/hasRatingValue">
        <rdfs:domain rdf:resource="http://purl.org/spar/bido/ReviewRatingValue"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">The value of the rating given to a review.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label xml:lang="en">has rating value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://data.semanticweb.org/ns/swc/ontology#AcademicEvent -->

    <owl:Class rdf:about="http://data.semanticweb.org/ns/swc/ontology#AcademicEvent">
        <rdfs:comment>Any academic event such as conferences, workshops, symposia.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://data.semanticweb.org/ns/swc/ontology"/>
        <rdfs:label>academic event</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/bido/BibliometricMeasure -->

    <owl:Class rdf:about="http://purl.org/spar/bido/BibliometricMeasure"/>
    


    <!-- http://purl.org/spar/bido/CategorialBibliometricData -->

    <owl:Class rdf:about="http://purl.org/spar/bido/CategorialBibliometricData"/>
    


    <!-- http://purl.org/spar/bido/ConferenceRanking -->

    <owl:Class rdf:about="http://purl.org/spar/bido/ConferenceRanking">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
        <rdfs:comment>This class describes conference rankings as composed by categories identifying different rankings.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label>conference ranking</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/bido/ConferenceRankingCategory -->

    <owl:Class rdf:about="http://purl.org/spar/bido/ConferenceRankingCategory">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/bido/CategorialBibliometricData"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="http://purl.org/spar/bido/withBibliometricData"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty>
                            <rdf:Description>
                                <owl:inverseOf rdf:resource="http://purl.org/spar/bido/holdsBibliometricDataInTime"/>
                            </rdf:Description>
                        </owl:onProperty>
                        <owl:allValuesFrom rdf:resource="http://data.semanticweb.org/ns/swc/ontology#AcademicEvent"/>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>This class identifies all the possible categories that can be used for ranking a conference according to the related ranking that defines them.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label>conference ranking category</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/bido/DiscreteReviewRatingScale -->

    <owl:Class rdf:about="http://purl.org/spar/bido/DiscreteReviewRatingScale">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/bido/ReviewRatingScale"/>
        <rdfs:comment xml:lang="en">A discrete scale defining the rating given by a reviewer for a specific scientific contribution.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label xml:lang="en">discrete review rating scale</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/bido/DiscreteReviewerConfidenceScale -->

    <owl:Class rdf:about="http://purl.org/spar/bido/DiscreteReviewerConfidenceScale">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/bido/ReviewerConfidenceScale"/>
        <rdfs:comment xml:lang="en">A discrete scale defining the confidence of a reviewer for a given topic.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label xml:lang="en">discrete reviewer confidence scale</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/bido/PeerReviewType -->

    <owl:Class rdf:about="http://purl.org/spar/bido/PeerReviewType">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/bido/CategorialBibliometricData"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="http://purl.org/spar/bido/withBibliometricData"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty>
                            <rdf:Description>
                                <owl:inverseOf rdf:resource="http://purl.org/spar/bido/holdsBibliometricDataInTime"/>
                            </rdf:Description>
                        </owl:onProperty>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://data.semanticweb.org/ns/swc/ontology#AcademicEvent"/>
                                    <rdf:Description rdf:about="http://purl.org/spar/fabio/Expression"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>This class describes different kinds of peer review processes, such as double-blind and open.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label>peer review type</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/bido/ReviewRatingScale -->

    <owl:Class rdf:about="http://purl.org/spar/bido/ReviewRatingScale">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
        <rdfs:comment xml:lang="en">A scale defining the rating given by a reviewer for a specific scientific contribution.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label xml:lang="en">review rating scale</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/bido/ReviewRatingValue -->

    <owl:Class rdf:about="http://purl.org/spar/bido/ReviewRatingValue">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/bido/CategorialBibliometricData"/>
        <rdfs:comment xml:lang="en">This class describes the set of values compliant with a given review rating scale.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label xml:lang="en">review rating value</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/bido/ReviewerConfidenceScale -->

    <owl:Class rdf:about="http://purl.org/spar/bido/ReviewerConfidenceScale">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
        <rdfs:comment xml:lang="en">A scale defining the confidence of a reviewer for a given topic.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label xml:lang="en">reviewer confidence scale</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/bido/ReviewerConfidenceValue -->

    <owl:Class rdf:about="http://purl.org/spar/bido/ReviewerConfidenceValue">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/bido/CategorialBibliometricData"/>
        <rdfs:comment xml:lang="en">This class describes the set of values compliant with a given reviewer confidence scale.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label xml:lang="en">reviewer confidence value</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/bido/ScholarlyVenueManagementSystem -->

    <owl:Class rdf:about="http://purl.org/spar/bido/ScholarlyVenueManagementSystem">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/bido/CategorialBibliometricData"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="http://purl.org/spar/bido/withBibliometricData"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty>
                            <rdf:Description>
                                <owl:inverseOf rdf:resource="http://purl.org/spar/bido/holdsBibliometricDataInTime"/>
                            </rdf:Description>
                        </owl:onProperty>
                        <owl:allValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://data.semanticweb.org/ns/swc/ontology#AcademicEvent"/>
                                    <rdf:Description rdf:about="http://purl.org/spar/fabio/Expression"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>This class describes all the possible venue management systems (such as conference management systems and journal management systems) that can be used for managing scholarly venues.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/bido-review-measures"/>
        <rdfs:label>scholarly venue management system</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/Expression -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/Expression">
        <rdfs:comment>Any possible work such as journals, magazines, books, etc., created for scientific purposes and possibily related with review processes.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/spar/fabio"/>
        <rdfs:label>expression</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/2004/02/skos/core#ConceptScheme -->

    <owl:Class rdf:about="http://www.w3.org/2004/02/skos/core#ConceptScheme">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    </owl:Class>
    


