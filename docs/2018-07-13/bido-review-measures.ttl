:to-10-continuos a :ReviewRatingScale,
        owl:NamedIndividual ;
    rdfs:label "0 to 10 continuos review rating scale"@en ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> .

:to-15-continuos a :ReviewRatingScale,
        owl:NamedIndividual ;
    rdfs:label "0 to 15 continuos review rating scale"@en ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> .

:to-5-continuos a :ReviewRatingScale,
        owl:NamedIndividual ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
