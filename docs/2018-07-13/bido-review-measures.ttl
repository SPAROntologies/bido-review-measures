skos:closeMatch a owl:ObjectProperty ;
    rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .

skos:prefLabel a owl:AnnotationProperty .

:ConferenceRanking a owl:Class ;
    rdfs:label "conference ranking" ;
    rdfs:comment "This class describes conference rankings as composed by categories identifying different rankings." ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    rdfs:subClassOf skos:ConceptScheme .

:ReviewerConfidenceScale a owl:Class ;
    rdfs:label "reviewer confidence scale"@en ;
    rdfs:comment "A scale defining the confidence of a reviewer for a given topic."@en ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    rdfs:subClassOf skos:ConceptScheme .

:core-a a :ConferenceRankingCategory,
        owl:NamedIndividual ;
    rdfs:label "A" ;
    :isDefinedIn :core-conference-ranking ;
    :isHigherThan :core-b ;
    rdfs:comment "The CORE category for an excellent conference, and highly respected in a discipline area." ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> .

:core-b a :ConferenceRankingCategory,
        owl:NamedIndividual ;
    rdfs:label "B" ;
    :isDefinedIn :core-conference-ranking ;
    :isHigherThan :core-c ;
    rdfs:comment "The CORE category for a good conference, and well regarded in a discipline area." ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> .

:core-c a :ConferenceRankingCategory,
        owl:NamedIndividual ;
    rdfs:label "C" ;
    :isDefinedIn :core-conference-ranking ;
    :isHigherThan :core-unranked ;
    rdfs:comment "The CORE category for other ranked conference venues that meet minimum standards." ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> .

:core-unranked a :ConferenceRankingCategory,
        owl:NamedIndividual ;
    rdfs:label "unranked" ;
    :isDefinedIn :core-conference-ranking ;
    rdfs:comment "The CORE category for a conference for which no ranking decision has been made." ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> .

:isLowerThan a owl:ObjectProperty ;
    rdfs:label "is lower than" ;
    rdfs:comment "It links a ranking category with one that is more important." ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> .

skos:inScheme a owl:ObjectProperty ;
    rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .

:DiscreteReviewerConfidenceScale a owl:Class ;
    rdfs:label "discrete reviewer confidence scale"@en ;
    rdfs:comment "A discrete scale defining the confidence of a reviewer for a given topic."@en ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    rdfs:subClassOf :ReviewerConfidenceScale .

:ScholarlyVenueManagementSystem a owl:Class ;
    rdfs:label "scholarly venue management system" ;
    rdfs:comment "This class describes all the possible venue management systems (such as conference management systems and journal management systems) that can be used for managing scholarly venues." ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Restriction ;
                    owl:allValuesFrom [ a owl:Class ;
                            owl:unionOf ( swc:AcademicEvent fabio:Expression ) ] ;
                    owl:onProperty [ owl:inverseOf :holdsBibliometricDataInTime ] ] ;
            owl:onProperty [ owl:inverseOf :withBibliometricData ] ],
        :CategorialBibliometricData .

:acl-1-5-acl1 a :ReviewerConfidenceValue,
        owl:NamedIndividual ;
    rdfs:label "1 out of 5 (Discrete 1 to 5 review rating scale)"@en ;
    :hasConfidenceValue "1"^^xsd:int ;
    rdfs:comment "Not reviewer's area of expertise, or paper is very hard to understand. The evaluation is just an educated guess."@en ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    skos:closeMatch :acl-1-5-acl1,
        :ex-null-null ;
    skos:inScheme :acl-1-5 .

:acl-1-5-acl2 a :ReviewerConfidenceValue,
        owl:NamedIndividual ;
    rdfs:label "2 out of 5 (Discrete 1 to 5 review rating scale)"@en ;
    :hasConfidenceValue "2"^^xsd:int ;
    rdfs:comment "Reviewers is willing to defend my evaluation, but it is fairly likely that he/she missed some details, didn't understand some central points, or can't be sure about the novelty of the work."@en ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    skos:closeMatch :acl-1-5-acl2,
        :ex-null-low ;
    skos:inScheme :acl-1-5 .

:acl-1-5-acl3 a :ReviewerConfidenceValue,
        owl:NamedIndividual ;
    rdfs:label "3 out of 5 (Discrete 1 to 5 review rating scale)"@en ;
    :hasConfidenceValue "3"^^xsd:int ;
    rdfs:comment "Reviewer is pretty sure, but there's a chance he/she missed something. Although she/he has a good feel for this area in general, she/he did not carefully check the paper's details, e.g., the math, experimental design, or novelty."@en ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    skos:closeMatch :acl-1-5-acl3,
        :ex-null-med ;
    skos:inScheme :acl-1-5 .

:acl-1-5-acl4 a :ReviewerConfidenceValue,
        owl:NamedIndividual ;
    rdfs:label "4 out of 5 (Discrete 1 to 5 review rating scale)"@en ;
    :hasConfidenceValue "4"^^xsd:int ;
    rdfs:comment "Reviewer is quite sure, tried to check the important points carefully. It's unlikely, though conceivable, that he/she missed something that should affect his/her ratings."@en ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    skos:closeMatch :acl-1-5-acl4,
        :ex-null-high ;
    skos:inScheme :acl-1-5 .

:acl-1-5-acl5 a :ReviewerConfidenceValue,
        owl:NamedIndividual ;
    rdfs:label "5 out of 5 (Discrete 1 to 5 review rating scale)"@en ;
    :hasConfidenceValue "5"^^xsd:int ;
    rdfs:comment "Reviewer is positive that the evaluation is correct, has read the paper very carefully and is familiar with related work."@en ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    skos:closeMatch :acl-1-5-acl5,
        :ex-null-exp ;
    skos:inScheme :acl-1-5 .

:dis-0-5-2 a :ReviewRatingValue,
        owl:NamedIndividual ;
    rdfs:label "2 out of 5 (Discrete 0 to 5 review rating scale)"@en ;
    :hasRatingValue "2"^^xsd:int ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    skos:closeMatch :sa-sr-bl-wrej,
        :sa-sr-nb-wrej ;
    skos:inScheme :dis-0-5 .

:ex-null-exp a :ReviewerConfidenceValue,
        owl:NamedIndividual ;
    rdfs:label "Expert (Expert to Null reviewer confidence scale)"@en ;
    :hasConfidenceValue "4"^^xsd:int ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    skos:closeMatch :acl-1-5-acl5,
        :ex-null-exp ;
    skos:inScheme :ex-null .

:ex-null-high a :ReviewerConfidenceValue,
        owl:NamedIndividual ;
    rdfs:label "High (Expert to Null reviewer confidence scale)"@en ;
    :hasConfidenceValue "3"^^xsd:int ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    skos:closeMatch :acl-1-5-acl4,
        :ex-null-high ;
    skos:inScheme :ex-null .

:ex-null-low a :ReviewerConfidenceValue,
        owl:NamedIndividual ;
    rdfs:label "Low (Expert to Null reviewer confidence scale)"@en ;
    :hasConfidenceValue "1"^^xsd:int ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    skos:closeMatch :acl-1-5-acl2,
        :ex-null-low ;
    skos:inScheme :ex-null .

:ex-null-med a :ReviewerConfidenceValue,
        owl:NamedIndividual ;
    rdfs:label "Medium (Expert to Null reviewer confidence scale)"@en ;
    :hasConfidenceValue "2"^^xsd:int ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    skos:closeMatch :acl-1-5-acl3,
        :ex-null-med ;
    skos:inScheme :ex-null .

:ex-null-null a :ReviewerConfidenceValue,
        owl:NamedIndividual ;
    rdfs:label "Null (Expert to Null reviewer confidence scale)"@en ;
    :hasConfidenceValue "0"^^xsd:int ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    skos:closeMatch :acl-1-5-acl1,
        :ex-null-null ;
    skos:inScheme :ex-null .

:sa-sr-bl-wrej a :ReviewRatingValue,
        owl:NamedIndividual ;
    rdfs:label "Weak Reject (Strong Reject to Strong Accept review rating scale)"@en ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    skos:closeMatch :dis-0-5-2,
        :sa-sr-nb-wrej ;
    skos:inScheme :sa-sr-bl .

:sa-sr-nb-wrej a :ReviewRatingValue,
        owl:NamedIndividual ;
    rdfs:label "Weak Reject (Strong Reject to Strong Accept, without Borderline review rating scale)"@en ;
    rdfs:isDefinedBy <http://purl.org/spar/bido-review-measures> ;
    skos:closeMatch :dis-0-5-2,
        :sa-sr-bl-wrej ;
    skos:inScheme :sa-sr-nb .

